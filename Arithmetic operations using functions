#Arthimetic operations using functions 
a=10
b=5
def add(a,b):
    return(a+b)
def sub(a,b):
    return(a-b)
def mul(a,b):
    return(a*b)
def div(a,b):
    return(a/b)
print("Additiona = ",add(a,b))
print("Subtraction = ",sub(a,b))
print("Multiplication = ",mul(a,b))
print("Division = ",div(a,b))
print("End of program.")
